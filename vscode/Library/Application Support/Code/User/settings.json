{
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.leader": "\\",
  "vim.insertModeKeyBindings": [
    {
      "before": [ "j", "k" ],
      "after": [ "<Esc>" ]
    },
  ],
  "vim.normalModeKeyBindings": [
    {
      "before": [ "<leader>", "d", "s" ],
      "commands": [ "workbench.action.gotoSymbol" ],
    },
    // open file explorer at current file
    {
      "before": [ "-" ],
      "commands": [ "workbench.files.action.showActiveFileInExplorer"]
    },
    // jupyter navigation
    {
      "before": [ "<C-j>" ],
      "commands": [ "notebook.focusNextEditor" ]
    },
    {
      "before": [ "<C-k>" ],
      "commands": [ "jupyter.gotoPrevCellInFile" ]
    },
    // focus next/previous opened file
    {
      "before": [ "<leader>", "n"],
      "commands": ["workbench.action.nextEditorInGroup"]
    },
    {
      "before": [ "<leader>", "p"],
      "commands": ["workbench.action.previousEditorInGroup"]
    },
    // harpoon like commands
    {
      "before": [ "<space>", "a"],
      "commands": ["workbench.action.openEditorAtIndex1"]
    },
    {
      "before": [ "<space>", "s"],
      "commands": ["workbench.action.openEditorAtIndex2"]
    },
    {
      "before": [ "<space>", "d"],
      "commands": ["workbench.action.openEditorAtIndex3"]
    },
    {
      "before": [ "<space>", "f"],
      "commands": ["workbench.action.openEditorAtIndex4"]
    },
    // lsp
    {
      "before": ["g", "r"],
      "commands": ["editor.action.goToReferences"]
    },
    {
      "before": ["<leader>", "r", "n"],
      "commands": ["editor.action.rename"]
    },
    {
      "before": ["g", "D"],
      "commands": ["editor.action.revealDeclaration"]
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": [ "<leader>", "c", "s"],
      "commands": [ "github.copilot.chat.attachSelection", "workbench.action.focusActiveEditorGroup" ],
    },
  ],
  "extensions.experimental.affinity": {
    "vscodevim.vim": 1
  },
  "editor.cursorBlinking": "solid",
  "vim.cursorStylePerMode.insert": "block",
  "vim.cursorStylePerMode.normal": "block",
  "vim.cursorStylePerMode.replace": "block",
  "vim.cursorStylePerMode.visualblock": "block",
  "vim.cursorStylePerMode.visualline": "block",
  "vim.enableNeovim": true,
  "vim.neovimPath": "/opt/homebrew/bin/nvim",
  "vim.neovimConfigPath": "~/.config/nvim/init.lua",
  "vim.neovimUseConfigFile": true,
  "editor.minimap.enabled": false,
  "vs-kubernetes": {},
  "terminal.integrated.fontSize": 18,
  "terminal.integrated.lineHeight": 1.2,
  "terminal.integrated.copyOnSelection": true,
  "editor.fontSize": 18,
  "editor.glyphMargin": 18,
  "workbench.editor.openPositioning": "last",
  "remote.autoForwardPortsSource": "hybrid",
  "terminal.integrated.enableMultiLinePasteWarning": false,
  // "workbench.fontAliasing": "auto",
  "explorer.autoReveal": false,
  "workbench.editor.showTabs": "none",
  "catppuccin.bracketMode": "monochromatic",
  "workbench.commandPalette.experimental.enableNaturalLanguageSearch": false,
  "editor.wordWrap": "off",
  "terminal.integrated.commandsToSkipShell": [ "language-julia.interrupt" ],
  "julia.symbolCacheDownload": true,
  "debug.console.fontSize": 14,
  "workbench.startupEditor": "none",
  "julia.enableTelemetry": false,
  "terminal.integrated.fontFamily": "\"JetBrains Mono\"",
  "notebook.output.minimalErrorRendering": true,
  "editor.fontWeight": "normal",
  "terminal.integrated.drawBoldTextInBrightColors": false,
  "python.analysis.autoImportCompletions": true,
  "python.analysis.completeFunctionParens": true,
  "catppuccin.italicComments": false,
  "catppuccin.italicKeywords": false,
  "jupyter.enableExtendedPythonKernelCompletions": true,
  "jupyter.interactiveWindow.codeLens.enableGotoCell": false,
  "editor.tokenColorCustomizations": {
    "[Vesper]": {
      "textMateRules": [
        {
          "scope": [ "string", ],
          "settings": { "foreground": "#FF8080" }
        }
      ]
    }
  },
  "python.analysis.aiCodeActions": {},
  "redhat.telemetry.enabled": true,
  "editor.lineNumbers": "relative",
  "github.copilot.enable": {
    "*": false,
    "plaintext": false,
    "markdown": false,
    "scminput": false,
    "json": false,
    "yamljson": false,
    "python": false
  },
  "catppuccin.colorOverrides": {},
  "extensions.ignoreRecommendations": true,
  "editor.fontFamily": "\"JetBrains Mono\"",
  "vim.highlightedyank.enable": true,
  "editor.selectionHighlight": false,
  "editor.occurrencesHighlight": "off",
  "json.format.enable": false,
  "editor.renderLineHighlight": "all",
  "git.openRepositoryInParentFolders": "never",
  "editor.suggestLineHeight": 18,
  "editor.suggestFontSize": 18,
  "editor.suggest.preview": true,
  "telemetry.feedback.enabled": false,
  "settingsSync.keybindingsPerPlatform": false,
  "workbench.editor.closeEmptyGroups": false,
  "window.autoDetectColorScheme": true,
  "github.copilot.chat.followUps": "never",
  "github.copilot.renameSuggestions.triggerAutomatically": false,
  "chat.implicitContext.enabled": {
    "panel": "never",
    "editing-session": "never"
  },
  "editor.inlayHints.enabled": "off",
  "git.blame.editorDecoration.enabled": false,
  "git.blame.statusBarItem.enabled": true,
  "github.copilot.advanced": {},
  "docker.extension.enableComposeLanguageServer": false,
  "editor.inlineSuggest.edits.allowCodeShifting": "never",
  "githubPullRequests.pullBranch": "never",
  "[dockercompose]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.autoIndent": "advanced",
    "editor.quickSuggestions": {
      "other": true,
      "comments": false,
      "strings": true
    },
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[github-actions-workflow]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "files.watcherExclude": {
    "**/.bloop": true,
    "**/.metals": true
  },
  "python.defaultInterpreterPath": "/Users/jaimeparedes/projects/Tala_Feature_Store/venv",
  "editor.inlineSuggest.edits.renderSideBySide": "never",
  "github.copilot.nextEditSuggestions.fixes": false,
  "github.copilot.nextEditSuggestions.allowWhitespaceOnlyChanges": false,
  "explorer.confirmDelete": false,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "workbench.preferredLightColorTheme": "Catppuccin Latte",
  "workbench.preferredDarkColorTheme": "Catppuccin Mocha",
  "docker.extension.dockerEngineAvailabilityPrompt": false,
}
